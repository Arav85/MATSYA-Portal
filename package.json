{
  "name": "fishery-portal-backend",
  "version": "1.0.0",
  "description": "Backend for the Fishery Data Collection Portal",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run start-api\" \"npm run start-server\"",
    "start-api": "json-server --watch database.json --port 3000",
    "start-server": "node server.js"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "json-server": "^0.17.4"
  }
}